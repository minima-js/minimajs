    "@types/uuid": "^11.0.0",
    "jest": "^30.2.0",
    "jest-util": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "zod": "^4.3.4"
  },
  "dependencies": {
    "@fastify/busboy": "^3.2.0",
    "uuid": "^13.0.0"
  },
  "peerDependencies": {
    "@minimajs/server": ">=0.3.0",
    "zod": "^4.3.4"
