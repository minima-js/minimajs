{
  "name": "@minimajs/multipart",
  "version": "1.1.0",
  "description": "Multipart/form-data handling with file upload support, validation, and schema integration",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./unstable": "./lib/unstable.js",
    "./helpers": "./lib/helpers.js",
    "./schema": "./lib/schema/index.js"
  },
  "scripts": {
    "dev": "tsc -w",
