{
  "name": "minimajs",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.5",
  "homepage": "https://minima-js.github.io",
  "repository": {
    "type": "github",
    "url": "git@github.com:minima-js/minima-js.github.io"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "lint": "bun eslint .",
    "format": "prettier --write --cache .",
    "coverage": "turbo coverage",
    "clean": "turbo clean",
    "release": "turbo build && changeset publish",
    "docs": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:api": "typedoc"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "mermaid": "^11.12.2",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "turbo": "^2.7.2",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitepress": "^1.6.4",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vue": "^3.5.26"
  }
}
